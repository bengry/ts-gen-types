{
  "name": "ts-gen-types",
  "description": "Generate TypeScript types from other types",
  "version": "0.0.0",
  "author": "Ben Grynhaus",
  "bin": {
    "ts-gen-types": "./bin/run"
  },
  "bugs": "https://github.com/bengry/ts-gen-types/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "ts-node": "^7.0.1",
    "tslib": "^1",
    "ts-simple-ast": "^13.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^1",
    "@types/node": "^10",
    "globby": "^8",
    "prettier": "^1.14.2",
    "tslint": "^5",
    "typescript": "^2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ts-gen-types",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bengry/ts-gen-types",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json package-lock.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
